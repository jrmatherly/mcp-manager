{
  "name": "fastmcp-manager-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "FastMCP Manager Frontend",
  "author": "Jason Matherly",
  "license": "MIT",
  "homepage": "https://matherly.net",
  "repository": {
    "type": "git",
    "url": "https://github.com/jrmatherly/mcp-manager"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui --reporter=verbose",
    "typecheck": "tsc --noEmit",
    "db:setup": "tsx src/db/setup.ts",
    "db:setup:drop": "tsx src/db/setup.ts drop",
    "db:setup:create": "tsx src/db/setup.ts create",
    "db:setup:test": "tsx src/db/setup.ts test",
    "db:setup:verify": "tsx src/db/setup.ts verify",
    "db:setup:full": "npm run db:setup && npm run db:optimize",
    "db:reset:full": "npm run db:setup:drop && npm run db:setup:create && npm run db:migrate && npm run db:optimize",
    "db:generate": "drizzle-kit generate --config=drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config=drizzle.config.ts",
    "db:push": "drizzle-kit push --config=drizzle.config.ts",
    "db:studio": "drizzle-kit studio --config=drizzle.config.ts",
    "db:seed": "tsx src/db/migrate.ts seed",
    "db:reset": "tsx src/db/migrate.ts reset",
    "db:reset-and-seed": "tsx src/db/migrate.ts reset-and-seed",
    "db:introspect": "drizzle-kit introspect --config=drizzle.config.ts",
    "db:optimize": "tsx src/db/optimize.ts",
    "db:health": "tsx src/db/optimize.ts health-check",
    "db:maintenance": "tsx src/db/optimize.ts maintenance",
    "db:analyze": "tsx src/db/optimize.ts analyze"
  },
